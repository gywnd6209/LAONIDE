.triangle-border:before {
    border-color: #5a8f00 transparent;
    border-style: solid;
    border-width: 20px 20px 0;
    bottom: -20px;
    content: "";
    display: block;
    left: 40px;
    position: absolute;
    width: 0;
}
.triangle-border.left {
	margin-top: 0;
	background:#ffffff;
	border: 1px solid #dedede;
}
.triangle-border.left:before {
	border-color: transparent #ccc;
	border-width: 4px 8px 4px 0;
	bottom: auto;
	left: -9px;
	top: 7px;
}
.triangle-border.left:after {
	border-color: transparent #fff;
    border-width: 4px 8px 4px 0;
    bottom: auto;
    left: -8px;
    top: 7px;
}
.triangle-border.right {
	background: #98defb;
    border-color: 1px solid #81daff;
    float: right;
    margin-right: 30px;
	
}
.triangle-border.right:before {
    border-color: transparent #81daff;
    border-width: 4px 0 4px 8px;
    bottom: auto;
    left: auto;
    right: -8px;
    top: 7px;
}
.triangle-border.right:after {
	border-color: transparent #98defb;
    border-width: 4px 0 4px 8px;
    bottom: auto;
    left: auto;
    right: -8px;
    top: 7px;
}

