/************/
/* nav-tabs */
/************/
.nav-tabs > li:before {
    -webkit-box-shadow: 3px 0 0 0 #eee;
            box-shadow: 3px 0 0 0 #eee;
    border-right: 1px solid #dbdbdb;
}
.nav-tabs > li:after {
    -webkit-box-shadow: -3px 0 0 0 #eee;
            box-shadow: -3px 0 0 0 #eee;
    border-left: 1px solid #dbdbdb;
}
.nav-tabs > li.active:before {
    -webkit-box-shadow: 2px 0 0 0 #fff;
            box-shadow: 2px 0 0 0 #fff;
    border-bottom: 1px solid #ddd;
    border-right: 1px solid #ddd;
}
.nav-tabs > li.active:after {
    -webkit-box-shadow: -2px 0 0 0 #fff;
            box-shadow: -2px 0 0 0 #fff;
    border-bottom: 1px solid #ddd;
    border-left: 1px solid #ddd;
}
.nav-tabs > li > a {
	background-color: #eee;
	color: #aaa;
	border-top: 1px solid #dbdbdb;
}
.nav-tabs > li > a:before {
	background: #eee;
	border-left: 1px solid #dbdbdb;
	border-top: 1px solid #dbdbdb;
}
.nav-tabs > li > a:after {	
	background: #eee;
	border-right: 1px solid #dbdbdb;
	border-top: 1px solid #dbdbdb;
}
.nav-tabs > li > a,
.nav-tabs > li > a:before,
.nav-tabs > li > a:after { 
    background: -webkit-gradient(linear, left top, left bottom, color-stop(80%, #f4f4f4), to(#e8e8e8)); 
    background: -webkit-linear-gradient(#f4f4f4 80%, #e8e8e8 100%); 
    background: -o-linear-gradient(#f4f4f4 80%, #e8e8e8 100%); 
    background: linear-gradient(#f4f4f4 80%, #e8e8e8 100%);
}
.nav-tabs > li.active > a,
.nav-tabs > li.active > a:hover,
.nav-tabs > li.active > a:focus {
	margin-bottom: -1px;
/* 	box-shadow: 0 -1px 4px -3px #ccc,
				-3px -2px 4px -3px #ccc,
				3px -2px 4px -3px #ccc; */
	border-top: 1px solid #ddd;
	border-bottom: 0;
}
.nav-tabs > li.active > a,
.nav-tabs > li.active > a:hover,
.nav-tabs > li.active > a:focus {
 	-webkit-box-shadow: 0 0 0 0 #ccc;
    box-shadow: 0 0 0 0 #ccc;
	background: #fff;
}
.nav-tabs > li.active > a:before {
	background: #fff;
	border-left: 1px solid #ddd;
	border-top: 1px solid #ddd;
}
.nav-tabs > li.active > a:after {	
	background: #fff;
	border-right: 1px solid #ddd;
	border-top: 1px solid #ddd;
}
.nav-tabs > li.active > #gLayoutTab_Terminal:before,
.nav-tabs > li.active > #gLayoutTab_Terminal:after {
	background: rgb(30,30,30);
}
.nav-tabs > #gLayoutLi_Terminal.active:before {
	-webkit-box-shadow: 3px 0 0 0 rgb(30,30,30);
	        box-shadow: 3px 0 0 0 rgb(30,30,30);
}
.nav-tabs > #gLayoutLi_Terminal.active:after {
	-webkit-box-shadow: -3px 0 0 0 rgb(30,30,30);
	        box-shadow: -3px 0 0 0 rgb(30,30,30);
}



