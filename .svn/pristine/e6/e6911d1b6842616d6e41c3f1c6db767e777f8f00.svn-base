package kr.or.ddit.vo;

public class SolboardVO {
	private String sol_no      ;
	private String sol_cont    ;
	private String sol_writer  ;
	private String sol_del_ok  ;
	private String err_no      ;
	private String sol_ok      ;
	private String sol_reg_date;
	private String mem_nick;
	
	public String getMem_nick() {
		return mem_nick;
	}
	public void setMem_nick(String mem_nick) {
		this.mem_nick = mem_nick;
	}
	public String getSol_no() {
		return sol_no;
	}
	public void setSol_no(String sol_no) {
		this.sol_no = sol_no;
	}
	public String getSol_cont() {
		return sol_cont;
	}
	public void setSol_cont(String sol_cont) {
		this.sol_cont = sol_cont;
	}
	public String getSol_writer() {
		return sol_writer;
	}
	public void setSol_writer(String sol_writer) {
		this.sol_writer = sol_writer;
	}
	public String getSol_del_ok() {
		return sol_del_ok;
	}
	public void setSol_del_ok(String sol_del_ok) {
		this.sol_del_ok = sol_del_ok;
	}
	public String getErr_no() {
		return err_no;
	}
	public void setErr_no(String err_no) {
		this.err_no = err_no;
	}
	public String getSol_ok() {
		return sol_ok;
	}
	public void setSol_ok(String sol_ok) {
		this.sol_ok = sol_ok;
	}
	public String getSol_reg_date() {
		return sol_reg_date;
	}
	public void setSol_reg_date(String sol_reg_date) {
		this.sol_reg_date = sol_reg_date;
	}
	
	
}
