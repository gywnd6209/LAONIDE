/* keyframes */

.do_spin_rev {
    -webkit-animation: do_spin_rev 1.8s infinite linear reverse;
    animation: do_spin 1.8s infinite linear reverse;
}

@-webkit-keyframes do_spin_rev {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100% {
        -webkit-transform: rotate(359deg);
        transform: rotate(359deg);
    }
}

@keyframes do_spin_rev {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100% {
        -webkit-transform: rotate(359deg);
        transform: rotate(359deg);
    }
}

@-webkit-keyframes sk-stretchdelay {
    0%,
    40%,
    100% {
        -webkit-transform: scaleY(0.4)
    }
    20% {
        -webkit-transform: scaleY(1.0)
    }
}

@keyframes sk-stretchdelay {
    0%,
    40%,
    100% {
        transform: scaleY(0.4);
        -webkit-transform: scaleY(0.4);
    }
    20% {
        transform: scaleY(1.0);
        -webkit-transform: scaleY(1.0);
    }
}

@-webkit-keyframes loading_dot {
    0%,
    80%,
    100% {
        -webkit-box-shadow: 0 30px 0 -15px;
        box-shadow: 0 30px 0 -15px;
    }
    40% {
        -webkit-box-shadow: 0 30px 0 0;
        box-shadow: 0 30px 0 0;
    }
}

@keyframes loading_dot {
    0%,
    80%,
    100% {
        -webkit-box-shadow: 0 30px 0 -15px;
        box-shadow: 0 30px 0 -15px;
    }
    40% {
        -webkit-box-shadow: 0 30px 0 0;
        box-shadow: 0 30px 0 0;
    }
}

