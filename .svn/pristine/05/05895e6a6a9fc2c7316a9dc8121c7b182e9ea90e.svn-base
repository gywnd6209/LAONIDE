/* dropdown */

.dropdown-menu>li>a {
    cursor: pointer;
}

.dropdown-menu>li>a:hover .helptext,
.dropdown-menu>li>a:focus .helptext {
    color: white;
}

.dropdown-menu .btn {
    background-color: transparent;
    margin-top: -5px;
    margin-right: 4px;
}

.dropdown-submenu {
    position: relative;
}

.dropdown-submenu>.dropdown-menu {
    -webkit-border-radius: 0;
    border-radius: 0;
    left: 100%;
    margin-left: -1px;
    margin-top: -6px;
    top: 0;
}

.dropdown-submenu:hover>.dropdown-menu {
    display: block;
}

.dropdown-submenu>a:after {
    border-color: transparent;
    border-left-color: #ccc;
    border-style: solid;
    border-width: 5px 0 5px 5px;
    content: " ";
    display: block;
    float: right;
    height: 0;
    width: 0;
    right: 24px;
    top: 6px;
    position: absolute;
}

.dropdown-submenu:hover>a:after {
    border-left-color: #fff;
}

