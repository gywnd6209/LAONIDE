/* dictionary(자동완성) */

.dictionary_box {
    min-width: 190px;
    outline: none;
    position: absolute !important;
    max-width: 450px;
    z-index: 99;
    margin-top: 0;
    margin-left: 2px;
    padding: 0px;
}

.dictionary_box table {
    margin-bottom: 0 !important;
}

.dictionary_list {
    font-family: Times New Roman;
    max-height: 108px;
    min-height: 31px;
    min-width: 150px;
    overflow: auto;
    position: relative;
}

.dictionary_list .hovered {
    background-color: rgba(90, 147, 226, 0.3);
}

.dictionary_list .dictionary_element {
    font-size: 0;
    padding: 6px;
}

.dictionary_element {
    border-bottom: 1px solid gray;
    border-collapse: collapse;
    cursor: pointer;
    /* min-height: 26px; */
    overflow: hidden;
    padding-left: 2px;
    padding-right: 2px;
    white-space: nowrap;
}

.dictionary_element.center {
    border: 0px;
    height: 31px;
    display: -webkit-box;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    -ms-flex-align: center;
    align-items: center;
}

.dictionary_list_table {
    border: none;
    overflow-x: hidden;
    overflow-y: auto;
}

.dictionary_icon {
    min-width: 20px;
    display: inline-block;
    -webkit-box-flex: initial;
    -webkit-flex: initial;
    -ms-flex: initial;
    flex: initial;
}

.dictionary_name {
    display: inline-block;
    -webkit-box-flex: 0;
    -webkit-flex-grow: 0;
    -ms-flex-positive: 0;
    flex-grow: 0;
    overflow: hidden;
    -o-text-overflow: ellipsis;
    text-overflow: ellipsis;
    margin-right: 15px;
}

.dictionary_return {
    display: inline-block;
    -webkit-box-flex: 1;
    -webkit-flex-grow: 1;
    -ms-flex-positive: 1;
    flex-grow: 1;
    text-align: right;
}

.dictionary_document_box {
    position: absolute;
    min-width: 200px;
    max-width: 400px;
    min-height: 150px;
    max-height: 250px;
    z-index: 99;
    border: solid 1px;
    padding: 10px;
    white-space: pre-wrap;
    overflow-y: auto;
    font-size: 12px;
}

