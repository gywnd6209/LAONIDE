/**********
Optimized:Start
**********/

.do_spin {
    -webkit-animation: do_spin 2.8s infinite linear;
    animation: do_spin 2.8s infinite linear;
}

@-webkit-keyframes do_spin {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100% {
        -webkit-transform: rotate(359deg);
        transform: rotate(359deg);
    }
}

@keyframes do_spin {
    0% {
        -webkit-transform: rotate(0deg);
        transform: rotate(0deg);
    }
    100% {
        -webkit-transform: rotate(359deg);
        transform: rotate(359deg);
    }
}

