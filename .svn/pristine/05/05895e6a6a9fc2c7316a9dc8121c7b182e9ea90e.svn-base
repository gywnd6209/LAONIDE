    right: -4px;
    z-index: -1;
    border-bottom: none;
    -webkit-border-radius: 0 4px 0 0;
    border-radius: 0 4px 0 0;
    /* 	box-shadow: 0 2px hsla(0,0%,100%,.5) inset, 0 -6px 10px -2px #1a1a1a, 0 -1px 4px -2px #111, -1px 0 4px -2px #111, 1px 0 4px -2px #111; */
    -webkit-transform: skew(20deg, 0deg);
    -ms-transform: skew(20deg, 0deg);
    transform: skew(20deg, 0deg);
    width: 51%;
}

#south_tab>li.active>a,
#south_tab>li.active>a:hover,
#south_tab>li.active>a:focus {
    z-index: 1;
    margin-top: 0;
    margin-bottom: -3px;
    margin-left: 0;
    padding-top: 4px;
    padding-bottom: 2px;
}

#south_tab>li.active>a:before {
    content: '';
    /* To generate the box */
    position: absolute;
    top: -1px;
    bottom: 0;
    left: -4px;
    z-index: -1;
    border-bottom: none;
    -webkit-border-radius: 4px 0 0 0;
    border-radius: 4px 0 0 0;
    -webkit-transform: skew(-20deg, 0deg);
    -ms-transform: skew(-20deg, 0deg);
    transform: skew(-20deg, 0deg);
    width: 51%;
}

#south_tab>li.active>a:after {
    content: '';
    /* To generate the box */
    position: absolute;
    top: -1px;
    bottom: 0;
    right: -4px;
    z-index: -1;
    border-bottom: none;
    -webkit-border-radius: 0 4px 0 0;
    border-radius: 0 4px 0 0;
    -webkit-transform: skew(20deg, 0deg);
    -ms-transform: skew(20deg, 0deg);
    transform: skew(20deg, 0deg);
    width: 50%;
}

