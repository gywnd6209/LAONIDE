/* LECTURE PLUGIN CLOCK CSS */

#clock {
    -webkit-border-radius: 5px;
    border-radius: 5px;
    float: right;
    margin-top: 1px;
    max-width: 145px;
    position: relative;
    zoom: 0.8;
}

#clock:after {
    -webkit-border-radius: 100%;
    border-radius: 100%;
    bottom: 2px;
    content: '';
    height: 20px;
    left: 50%;
    margin-left: -200px;
    position: absolute;
    width: 400px;
    z-index: -1;
}

#clock .display {
    -webkit-border-radius: 6px;
    border-radius: 6px;
    height: 38px;
    padding: 5px;
    position: relative;
    text-align: center;
}

#clock .clock_guide {
    -webkit-border-radius: 5px;
    border-radius: 5px;
    border: 1px solid #ddd;
    display: none;
    font-size: 18px;
    padding: 5px;
}

/*-------------------------
    Light color theme
--------------------------*/

#clock.light {
    background-color: #f3f3f3;
    color: #272e38;
}

#clock.light:after {
    -webkit-box-shadow: 0 4px 10px rgba(0, 0, 0, 0.15);
    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.15);
}

#clock.light .digits div span {
    background-color: #272e38;
    border-color: #272e38;
}

#clock.light .digits div.dots:after,
#clock.light .digits div.dots:before {
    background-color: #272e38;
}

#clock.light .display {
    background-color: #ddd;
    -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.08) inset, 0 1px 1px #fafafa;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.08) inset, 0 1px 1px #fafafa;
}

/*-------------------------
    Dark color theme
--------------------------*/

#clock.dark {
    background-color: #272e38;
    color: #cacaca;
}

#clock.dark:after {
    -webkit-box-shadow: 0 4px 10px rgba(0, 0, 0, 0.3);
    box-shadow: 0 4px 10px rgba(0, 0, 0, 0.3);
}

#clock.dark .digits div span {
    background-color: #cacaca;
    border-color: #cacaca;
}

#clock.dark .display {
    background-color: #0f1620;
    -webkit-box-shadow: 0 1px 1px rgba(0, 0, 0, 0.08) inset, 0 1px 1px #2d3642;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.08) inset, 0 1px 1px #2d3642;
}

#clock.dark .digits div.dots:after,
#clock.dark .digits div.dots:before {
    background-color: #cacaca;
}

/*-------------------------
    The Digits
--------------------------*/

#clock .digits div {
    display: inline-block;
    height: 25px;
    margin: 0 2px;
    position: relative;
    text-align: left;
    width: 14px;
}

#clock .digits div span {
    -moz-transition: 0.25s;
    -o-transition: 0.25s;
    -webkit-transition: 0.25s;
    opacity: 0;
    position: absolute;
    transition: 0.25s;
}

#clock .digits div span:after,
#clock .digits div span:before {
    border: 5px solid transparent;
    content: '';
    height: 0;
    position: absolute;
    width: 0;
}

#clock .digits .d1 {
    height: 2.5px;
    left: 3px;
    top: 0;
    width: 8px;
}

#clock .digits .d1:before {
    border-right-color: inherit;
    border-width: 0 2.5px 2.5px 0;
    left: -2.5px;
}

#clock .digits .d1:after {
    border-left-color: inherit;
    border-width: 0 0 2.5px 3.5px;
    right: -2.5px;
}

#clock .digits .d2 {
    height: 2.5px;
    left: 3px;
    top: 12px;
    width: 8px;
}

#clock .digits .d2:before {
    border-right-color: inherit;
    border-width: 1.5px 2px 2px;
    left: -2px;
}

#clock .digits .d2:after {
    border-left-color: inherit;
    border-width: 2.5px 2.5px 0;
    right: -5px;
}

#clock .digits .d3 {
    height: 2.5px;
    left: 3px;
    top: 24px;
    width: 8px;
}

#clock .digits .d3:before {
    border-right-color: inherit;
    border-width: 2.5px 3.5px 0 0;
    left: -2.5px;
}

#clock .digits .d3:after {
    border-left-color: inherit;
    border-width: 2.5px 0 0 2.5px;
    right: -2.5px;
}

#clock .digits .d4 {
    height: 7px;
    left: 0;
    top: 3.5px;
    width: 2.5px;
}

#clock .digits .d4:before {
    border-bottom-color: inherit;
    border-width: 0 2.5px 2.5px 0;
    top: -2.5px;
}

#clock .digits .d4:after {
    border-left-color: inherit;
    border-width: 0 0 2.5px 3.5px;
    bottom: -2.5px;
}

#clock .digits .d5 {
    height: 7px;
    right: 0;
    top: 3.5px;
    width: 2.5px;
}

#clock .digits .d5:before {
    border-bottom-color: inherit;
    border-width: 0 0 2.5px 2.5px;
    top: -2.5px;
}

#clock .digits .d5:after {
    border-top-color: inherit;
    border-width: 2.5px 0 0 2.5px;
    bottom: -2.5px;
}

#clock .digits .d6 {
    height: 7px;
    left: 0;
    top: 16px;
    width: 2.5px;
}

#clock .digits .d6:before {
    border-bottom-color: inherit;
    border-width: 0 2.5px 2.5px 0;
    top: -2.5px;
}

#clock .digits .d6:after {
    border-left-color: inherit;
    border-width: 0 0 2.5px 2.5px;
    bottom: -2.5px;
}

#clock .digits .d7 {
    height: 7px;
    right: 0;
    top: 16px;
    width: 2.5px;
}

#clock .digits .d7:before {
    border-bottom-color: inherit;
    border-width: 0 0 2.5px 2.5px;
    top: -2.5px;
}

#clock .digits .d7:after {
    border-top-color: inherit;
    border-width: 2.5px 0 0 2.5px;
    bottom: -2.5px;
}

#clock .digits div.eight .d1,
#clock .digits div.eight .d2,
#clock .digits div.eight .d3,
#clock .digits div.eight .d4,
#clock .digits div.eight .d5,
#clock .digits div.eight .d6,
#clock .digits div.eight .d7,
#clock .digits div.five .d1,
#clock .digits div.five .d2,
#clock .digits div.five .d3,
#clock .digits div.five .d4,
#clock .digits div.five .d7,
#clock .digits div.four .d2,
#clock .digits div.four .d4,
#clock .digits div.four .d5,
